{
  "graph": {
    "type": "easel.mix",
    "label": {
      "template": "data/graph_label.template"
    },
    "mix": [{
      "type": "bar",
      "yLength": 1,
      "chartColorsAlpha": [0.1]
    }, {
      "type": "motionLine",
      "yLength": 1,
      "lineWidth": 8
    }],
    "data": "data/graph_data.json",
    "yLength": 2,
    "lineWidth": 6,
    "pointSize": 6,
    "chartColors": ["#666666", "#FFBE00"]
  },
  "range": {
    "dataType": "timeline",
    "unit": "monthly",
    "maxLength": 90,
    "length": 10
  }
}
