{
  "graph": {
    "type": "morris.line",
    "label": {
      "template": "/spec/graph_label.template"
    },
    "staticPath": {{{basePath}}},
    "data": "/spec/graph_data.json",
    "yLength": 2,
    "lineWidth": 6,
    "pointSize": 6,
    "autoResize": true
  },
  "slider": {
    "appendSliderAmountBottom": false,
    "hideSliderAmount": false
  },
  "list": {
    "staticPath": {{{basePath}}},
    "data": "/spec/list_data.json",
    "template": "/spec/list_data.template",
    "dataLabel": "title"
  },
  "range": {
    "dataType": "timeline",
    "unit": "daily"
  }
}
